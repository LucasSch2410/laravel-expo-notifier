{
  "name": "yieldstudio/laravel-expo-notifications",
  "description": "Easily send Expo notifications with Laravel.",
  "type": "plugin",
  "keywords": [
    "yieldstudio",
    "laravel",
    "notifier",
    "notification",
    "expo"
  ],
  "homepage": "https://github.com/YieldStudio/laravel-expo-notifications",
  "license": "MIT",
  "authors": [
    {
      "name": "James Hemery",
      "email": "james@yieldstudio.fr",
      "homepage": "https://yieldstudio.fr",
      "role": "Developer"
    },
    {
      "name": "David Tang",
      "email": "david@yieldstudio.fr",
      "homepage": "https://yieldstudio.fr",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/database": "^9",
    "illuminate/support": "^9"
  },
  "require-dev": {
    "ciareis/bypass": "^1.0",
    "dg/bypass-finals": "^1.4",
    "friendsofphp/php-cs-fixer": "^3.8",
    "orchestra/testbench": "7.*",
    "pestphp/pest": "^1.21",
    "phpunit/phpunit": "^9.1"
  },
  "autoload": {
    "psr-4": {
      "YieldStudio\\LaravelExpoNotifications\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "YieldStudio\\LaravelExpoNotifications\\Tests\\": "tests"
    }
  },
  "scripts": {
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
    "test": "vendor/bin/pest",
    "post-autoload-dump": [
      "@php ./vendor/bin/testbench package:discover --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "YieldStudio\\LaravelExpoNotifications\\ExpoNotificationsServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
